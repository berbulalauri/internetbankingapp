
@{
    ViewData["Title"] = "Order";
}

<div class="container">
    <h1 class="text-center">Profile and Settings</h1>
    <form class="card p-2">
        <div class="form-group row">
            <label for="newquestion" class="col-sm-2 col-form-label"> Add new question</label>
            <div class="col-sm-auto">

            </div>
        </div>


        <div class="form-group row">
            <div class="col-sm-auto">
                    <select id="DeffaultAccount" class="form-control" asp-items="ViewBag.Questions"></select>
            </div>
        </div>

        <div class="form-group row">
            <label for="Answer" class="col-sm-2 col-form-label">Answer</label>
            <div class="col-sm-auto">

            </div>
        </div>

        <div class="form-group row">

            <div class="col-sm-auto">
                <input type="text" class="form-control" id="answer" placeholder="">
            </div>
        </div>

        <div class="form-group row">
            <div class="col-sm-10">
                <input type="submit" id="addquestion" value="Add" >
            </div>
        </div>







     



    </form>
   
</div>



